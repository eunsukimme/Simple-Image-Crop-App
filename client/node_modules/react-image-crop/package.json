{
  "name": "react-image-crop",
  "version": "8.0.5",
  "description": "A responsive image cropping tool for React",
  "repository": "https://github.com/DominicTobias/react-image-crop",
  "main": "dist/ReactCrop.js",
  "style": "dist/ReactCrop.css",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack --progress --colors --watch --config demo/webpack.config.js & npm run sass-watch",
    "sass-watch": "node-sass lib/ReactCrop.scss dist/ReactCrop.css --watch",
    "build:js": "webpack",
    "build:css": "node-sass lib/ReactCrop.scss dist/ReactCrop.css",
    "build": "npm run build:js && npm run build:css"
  },
  "author": "Dominic Tobias <dominic.tobias@gmail.com>",
  "keywords": [
    "react",
    "reactjs",
    "image",
    "crop",
    "react-component"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "peerDependencies": {
    "react": ">=15.2.2"
  },
  "dependencies": {
    "clsx": "^1.0.4",
    "core-js": "^3.0.1",
    "prop-types": "^15.7.2"
  }
}
